; This program displays the arrays components through registers

INCLUDE Irvine32.inc

SUN = 0
MON = 1 
TUE = 2
WED = 3
THUR = 4
FRI = 5
SAT = 6

.data

days byte 0,1,2,3,4,5,6
.code

main PROC

movzx edx, days[1]
movzx ecx, days[2]
movzx ebx, days[3]
movzx eax, days[4]


call DumpRegs
	exit
main ENDP
END main
